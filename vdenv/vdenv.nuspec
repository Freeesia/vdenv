<package>
  <metadata>
    <id>vdenv</id>
    <version>$version$</version>
    <authors>$authors$</authors>
    <description>$Description$</description>
    <repository type="$RepositoryType$" url="$RepositoryUrl$" />
    <projectUrl>$PackageProjectUrl$</projectUrl>
    <tags>$PackageTags$</tags>
    <packageTypes>
      <packageType name="DotnetTool" />
    </packageTypes>
    <license type="expression">$PackageLicenseExpression$</license>
    <readme>$PackageReadmeFile$</readme>
  </metadata>

  <files>
    <file src="DotnetToolSettings.xml" target="tools\net8.0\any\" />
    <file src="bin\$configuration$\**\*" target="tools\net8.0\any\" />
    <file src="..\README.md" target="\" />
  </files>
</package>
